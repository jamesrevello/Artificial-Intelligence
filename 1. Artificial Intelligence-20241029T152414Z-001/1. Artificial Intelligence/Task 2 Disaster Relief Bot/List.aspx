    <script type="text/javascript">
        var Panopto = Panopto || {};
        Panopto.list = Panopto.list || {};
        Panopto.list.showVideoPreview = false
        Panopto.list.intervalWhenHidden = 60
        Panopto.list.showAllTimeViewCounts = false

        Sys.onReady(function () {
            var redirectToLogin = function () {
                // Unauthenticated request will redirect to the login page.
                if (Panopto.features.panoptoState == Panopto.Core.Constants.PanoptoStateRedirectParamKey)
                {
                    Panopto.Application.setPanoptoState(location.href)
                        .then((result) =>
                        {
                            var loginWithReturnUrl = Panopto.loginUrl + "?panoptoState=" + result?.panoptoStateId;
                            location.href = Panopto.teamsApp?.isFromTeams
                                ? loginWithReturnUrl + "&isFromTeams=true"
                                : loginWithReturnUrl;
                        });
                }
                else
                {
                    var loginWithReturnUrl = Panopto.loginUrl + "?ReturnUrl=" + encodeURIComponent(location.href);
                    location.href = Panopto.teamsApp?.isFromTeams
                        ? loginWithReturnUrl + "&isFromTeams=true"
                        : loginWithReturnUrl;
                }
            }

            // If user is not authenticated and request comes from TeamsApp redirect to login page
            if (!Panopto.user.isAuthenticated && Panopto.teamsApp?.isFromTeams) {
                redirectToLogin();
            }

            // Enable a generic "My Folder" request via hash param at load time.
            // Replace this temporary state with the actual folderID setting before loading SessionList().
            if (window.location.hash.indexOf("#isMyFolder=true") !== -1)
            {
                // If the user is unauthenticated, redirect to login.
                if (!Panopto.user.isAuthenticated)
                {
                    redirectToLogin();
                }
                // If the user has a sandbox folder redirect to the folder page.
                else if (Panopto.user.sandboxFolderId)
                {
                    Panopto.Application.defaultInstance.updateState({
                        folderID: Panopto.user.sandboxFolderId,
                        // Clear temporary state (not used by SessionList()).
                        isMyFolder: null
                    });
                }
                // If the user does not have a sandbox folder redirect to the Home page.
                else
                {
                    window.location.href = Panopto.appRoot + "/Pages/Home.aspx";
                }
            }

            // Shared with Me mode is only available for authenticated users.
            // If the user is unauthenticated, redirect to login.
            if (   window.location.hash.indexOf("#isSharedWithMe=true") !== -1
                && !Panopto.user.isAuthenticated)
            {
                redirectToLogin();
            }

            // Can't use Site.Master until in the $(document).ready
            Panopto.features.useLtiPostMessageStorageForAuthentication = false;

            PanoptoCore.LTI.setToken();

            var sessionList = new Panopto.SessionList({
                navBarInstance: Panopto.NavBar.defaultInstance,
                // Create menu may be in the header (embedded view), nav bar (regular view) or neither (no privileges).
                createMenuInstance:
                       Panopto.Application.defaultInstance.createMenu
                    ?? Panopto.NavBar.defaultInstance.createMenu,
                showCreatorTour: false,
                showViewerTour: false,
                showAdminSettingsPrompt: false,
                adminHelpSettingsUrl: '',
                showGoToParentButtonInEmbeddedSessionList: false,
                showSharedWithMeDateSort: true,
                defaultSharedWithMeDateSort: false,
                showSharedWithMeSearchSort: true
            });
            sessionList.init();
        });
    </script>

