                        <div class="item-tags" style="display:none"></div>

                        
                        <div class="item-subtitle"
                            sys:if="{{ArchiveEstimatedRestoreTime}}">
                                {{Panopto.Util.displayArchiveEstimatedRestoreTime(
                                    ArchiveRestoreRequestedTime,
                                    ArchiveEstimatedRestoreTime)}}
                        </div>

                        
                        <div class="item-description"
                                sys:title="{{ Panopto.Core.TextHelpers.cleanTextWithoutHighlighting(Abstract) }}"
                                sys:innerhtml="{{
                                Panopto.Core.TextHelpers.displayLineBreaks(
                                    Panopto.Core.TextHelpers.hasMatch(Abstract)
                                        ? Panopto.Core.TextHelpers.trimTextToMatch(
                                            Abstract, Panopto.Core.Constants.SessionAbstractResultsCharacterLimit)
                                        : Panopto.Core.TextHelpers.cleanTextWithoutHighlighting(
                                            Abstract),
                                    { replacement: ' &nbsp; ', collapseMultiple: true }
                                )
                                }}"
                                sys:if="{{ Abstract }}">
                        </div>

                        <div class="item-contents">
                            
                            <span sys:if="{{RemoteRecorderName && (IsCreator || Panopto.user.sitePermissions[PanoptoCore.Permission.SessionCreateScheduledRecording]) && Panopto.SessionList.displayRemoteRecorder(Status)}}"
                                    class="detail-context ellipsis">
                                <span>Remote Recorder:</span>
                                <a href="#" onclick="return false;"
                                    class="rr-link"
                                    sys:if="Panopto.user.sitePermissions[PanoptoCore.Permission.RemoteRecorderPreview]"
                                    sys:command="showModal:RemoteRecorderPreview"
                                    sys:commandargument="{{ {id: RemoteRecorderID} }}"
                                    sys:innerhtml=
                                        "{{Panopto.Core.TextHelpers.cleanTextWithHighlighting(RemoteRecorderName)}}">
                                </a>
                                <span sys:if="!Panopto.user.sitePermissions[PanoptoCore.Permission.RemoteRecorderPreview]"
                                        sys:innerhtml=
                                            "{{Panopto.Core.TextHelpers.cleanTextWithHighlighting(RemoteRecorderName)}}">
                                </span>
                            </span>

                            <!-- Search contexts with the "In this session" message (show if there are bookmarks or search contexts with times -->
                            <div class="detail-context" sys:if="{{((Context.length && ContextsHaveTime) || Bookmarks.length)}}">In this session</div>

                            
                            <div class="status-message"></div>
                        </div>

                        <div class="search-results">
                            
                            <table sys:if="{{Context.length && !Bookmarks.length}}">
                                <tbody sys:attach="dataview"
                                        sys:id="{{'context' + DeliveryID}}"
                                        dataview:data="{{Context}}"
                                        class="sys-template">
                                    <tr class="search-item" sys:id="{{PublicId}}">
                                        
                                        <td class="time-cell">{binding Time, convert=displayDuration}</td>

                                        <td class="currently-playing">
                                            <i class="material-icons">play_arrow</i>
                                        </td>

                                        
                                        <td class="context-cell">
                                            <a sys:href="{{Panopto.Util.getViewerLinkWithQuery($context.parentContext.dataItem, Time)}}"
                                                target="_blank"
                                                sys:if="{{$context.parentContext.dataItem.ContextsHaveTime}}"
                                                class="search-link ellipsis"
                                                sys:innerhtml="{{Panopto.Core.TextHelpers.trimTextToMatch(Context, Panopto.Core.Constants.SessionListContextResultsCharacterLimit)}}">
                                            </a>
                                            <div sys:if="{{!$context.parentContext.dataItem.ContextsHaveTime}}"
                                                    sys:innerhtml="{{Panopto.Core.TextHelpers.trimTextToMatch(Context, Panopto.Core.Constants.SessionListContextResultsCharacterLimit)}}">
                                            </div>
                                        </td>
                                    </tr>
                                </tbody>
                            </table>

                            
                            <table sys:if="{{Bookmarks.length}}">
                                <tbody id="bookmarks"
                                        sys:attach="dataview"
                                        dataview:data="{{Bookmarks}}"
                                        class="sys-template">
                                    <tr class="search-item">
                                        
                                        <td class="time-cell">{binding Time, convert=displayDuration,showZero=true}</td>

                                        
                                        <td class="context-cell">
                                            <div class="bookmark">
                                                <a sys:href="{{Panopto.Util.getViewerLinkWithQuery($context.parentContext.dataItem, Time)}}"
                                                    target="_blank"
                                                    class="search-link ellipsis"
                                                    sys:innerhtml="{{Panopto.Core.TextHelpers.cleanTextWithHighlighting(Name)}}"></a>
                                                <a href="#"
                                                    class="bookmark-delete"
                                                    title="Delete bookmark"
                                                    sys:onclick="{{ 'Panopto.SessionList.defaultInstance._deleteBookmark(&quot;' + PublicID + '&quot;, &quot;' + $context.parentContext.dataItem.SessionID + '&quot;); return false;' }}"></a>
                                            </div>
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </td>
                </tr>
            </tbody>
        </table>

        
        

    
    <ul id="gridViewItemTemplate" class="sys-template"><li class="item" sys:id="{{$dataItem.DeliveryID}}">

            
            <div class="thumbnail-wrapper">
                <div class="react-thumbnail-container"></div>
            </div>

            
            <div class="details">

                
                <div class="item-title title-link ellipsis"
                     sys:style="{{Panopto.Util.isViewable($dataItem.Status) ? '' : 'display:none'}}">
                    
                    <a sys:href="{{ Panopto.Util.getViewerLinkWithQuery($dataItem) }}"
                       class="detail-title"
                       target="_blank">
                    </a>
                    <span class="detail-title"
                        sys:innerhtml="{{Panopto.Util.displayDeliveryName($dataItem)}}">
                    </span>
                </div>
                <div class="item-title title-span ellipsis"
                     sys:style="{{Panopto.Util.isViewable($dataItem.Status) ? 'display:none' : ''}}">
                    
                    <span class="detail-title"
                          sys:innerhtml="{{Panopto.Util.displayDeliveryName($dataItem)}}"></span>
                </div>

                
                <div class="presenter ellipsis"
                     sys:if="{{ Panopto.Util.displayPresenterMessage($dataItem) }}"
                     style="display: none">
                    {{ Panopto.Util.displayPresenterMessage($dataItem) }}
                </div>

                
                <div class="item-date"
                     sys:if="$dataItem.Status != Panopto.Data.SessionStatus.Created && Panopto.features.showThumbnailDate"
                     sys:title="{{ Panopto.Util.displayDate(StartTime, { format: 'long' }) }}">
                    {{Panopto.Util.getDateMessage($dataItem)}}
                </div>

                
                <div class="parent-folder-link folder-link-container ellipsis"
                    sys:if="{{ !Panopto.features.showThumbnailDate && ($dataItem.TypeName || $dataItem.UserCanEnumerateFolder) }}">
                    <a href="#"
                        class="folder-link ellipsis"
                        sys:innerhtml="{{ $dataItem.FolderName ? Panopto.Core.TextHelpers.cleanTextWithHighlighting($dataItem.FolderName) : '&nbsp;' }}"></a>
                    <span class="folder-hover-container"></span>
                </div>
                <div class="folder-div ellipsis"
                        sys:if="{{ !Panopto.features.showThumbnailDate && !$dataItem.UserCanEnumerateFolder && !$dataItem.TypeName }}">
                        <span>
                            {{ Panopto.Core.TextHelpers.cleanTextWithHighlighting($dataItem.FolderName) }}
                        </span>
                </div>
            </div>
        </li></ul>

    </div>


            <div class="overlay"></div>
        </div>
    </div>


            <div id="loadingMessage" style="display: none"></div>

            <div id="logoutWarningMessage" class="safety-background" style="display: none;">
                <span id="logoutWarningText"></span>
                <span id="logoutWarningInfo" style="display: none;"></span>
                <a id="logoutWarningLink"></a>
            </div>
        </div>

        
<script type="text/javascript" src='//static-assets-cdn.i.hosted.panopto.com/Panopto/Cache/14.20.1.00001/Scripts/Panopto/Controls/ModalPopup.js'></script>


